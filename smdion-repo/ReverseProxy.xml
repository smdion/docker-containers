<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Reverse-Proxy</Name>
  <Description>
    Reverse Proxy is a Apache based web server set up to act as a proxy with basic PHP web server capabilities[br][br]
    [b][span style='color: #E80000;']Directions:[/span][/b][br]
    [b]/config[/b] : this path is used to store the configuration and the SSL certs files of the reverse proxy.[br]
    [b]/web[/b] : this path is the web directory[br]
  </Description>
  <Registry>https://registry.hub.docker.com/u/smdion/docker-reverseproxy/</Registry>
  <Repository>smdion/docker-reverseproxy</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>80</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>443</HostPort>
        <ContainerPort>443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/web</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
        <Volume>
      <HostDir></HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/etc/localtime</HostDir>
      <ContainerDir>/etc/localtime</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
    <WebUI></WebUI>
  <Banner>http://i.imgur.com/u4nQCdW.png</Banner>
  <Icon>http://i.imgur.com/M5pmW3X.png</Icon>
</Container>
